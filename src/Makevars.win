OBJECTS.vcfr = rvcf.o RcppExports.o vcf/txtReader.o \
    vcf/variantIndex.o \
    vcf/vcfReader.o \
    vcf/gzstream/gzstream.o \
    vcf/vcfReaderDebug.o

OBJECTS = $(OBJECTS.vcfr)
PKG_CXXFLAGS = -I/usr/share/R/include/ -Ivcf/ -Ivcf/gzstream/ -DVERSION="\"R\"" -DRBUILD -DSTRICT_R_HEADERS -Wdate-time -Werror=format-security -Wformat
PKG_LIBS = -lz
